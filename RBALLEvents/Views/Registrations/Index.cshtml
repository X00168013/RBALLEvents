@model IEnumerable<RBALLEvents.Models.Registration>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Division)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Event)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Member)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Division)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Event.RacquetballEventID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Member.Address)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RegistrationID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RegistrationID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RegistrationID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
